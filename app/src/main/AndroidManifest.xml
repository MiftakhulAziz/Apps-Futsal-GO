<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.futsalgo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_aplikasi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_aplikasi"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RekeningTransferActivity"></activity>
        <activity android:name=".BuatPesananActivity" />
        <activity android:name=".PilihTempatFutsalActivity" />
        <activity android:name=".MenuTempatFutsal.DetailTempatFutsalActivity" />
        <activity android:name=".MenuBeranda.ViewImageActivity" />
        <activity android:name=".DialogActivity" />
        <activity android:name=".Activities.UbahProfilActivity" />
        <activity android:name=".WelcomeActivity" />
        <activity
            android:name=".NavDrawerActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Authentikasi.LoginActivity" />
        <activity android:name=".Authentikasi.RegisterActivity" />
        <activity android:name=".Authentikasi.ForgotPasswordActivity" />
        <activity
            android:name=".SplashSreenActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>